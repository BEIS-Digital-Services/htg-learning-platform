@model Beis.LearningPlatform.Web.Models.FeedbackPromptViewModel
@if (Model.IsJavascriptEnabled ?? true)
{
    <partial name="CMSPageViews/_PartialFeedbackPromptJs" model="@Model.CmsPageComponent" />
}
else
{
    if (Model.IsFeedbackSubmitted)
    {
        <partial name="CMSPageViews/_PartialFeedbackPromptNoJsSubmitted" model="@Model.CmsPageComponent" />
    }
    else
    {
        <partial name="CMSPageViews/_PartialFeedbackPromptNoJs" model="@Model.CmsPageComponent" />
    }    
}