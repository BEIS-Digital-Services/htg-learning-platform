@model Beis.LearningPlatform.Web.StrapiApi.Models.CMSPageViewModel
<main id="main-heading" role="main">
    @if (Model.hero_banners != null)
    {
        <partial name="CMSPageViews/_PartialHeroBanner" model="@Model" />
    }

    @if (Model.side_navigations != null)
    {
        <partial name="CMSPageViews/_PartialSideNavigation" model="@Model.side_navigations" />
    }

    <partial name="CMSPageViews/_PartialRenderCMSComponents" model="@Model" />
</main>